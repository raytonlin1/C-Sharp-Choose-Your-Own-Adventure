using System;
					
public class Program
{
	public static void Main()
	{
		Console.Write("Do you want to leave your room? Type YES or NO: ");
  string firstChoice = Console.ReadLine().ToUpper(); 
    if (firstChoice == "NO") 
    {
      Console.WriteLine("There is no adventure in your room!");
      
    }
    else if (firstChoice == "YES") 
    {
      Console.WriteLine("There is no adventure outside of your room!");
     
    }
		Console.WriteLine("THE END.");
	}
}
